page{
  background: #fff;
}
.container {
  position: static;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}
.nav_left{
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
}
.nav_left .nav_left_items{
  height: 30px;
  line-height: 30px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}
.nav_left .nav_left_items.active{
  background: #fff;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  width: 75%;
  height: 100%;
  padding: 10px;
  padding-right: 0;
  background: #fff;
  box-sizing: border-box;
}
.nav_right .nav_right_items image{
  display:inline-block;
  text-align: center;
  margin-top: 15rpx;
  margin-left: 20rpx;
  padding: 0;
  height: 220rpx;
  width: 75%;
  border: 1rpx solid lightgrey;
}
.nav_right .nav_right_items .content{
  clear: left;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  margin-left: 20rpx;
  padding: 0;
  font-size: 0.5rem;
  height: 220rpx;
  width: 75%;
}